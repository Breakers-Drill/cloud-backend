generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SensorData {
  id        Int      @id @default(autoincrement())
  edgeId    Int      @map("edge_id")
  timestamp DateTime @default(now())
  tag       String
  value     Int

  @@map("sensor_data")
}
